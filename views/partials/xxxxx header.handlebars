<script type="text/javascript">
  var progressBar = new ProgressBar.Line('#headerload', {
    easing: 'easeInOut',
    from: {color: '#ef4e7b'},
    to: {color: '#0366d6'},
    duration: 1400,fill: 'rgba(0, 0, 0, 0.5)',
    step: (state, bar) => {
      bar.path.setAttribute('stroke', state.color);
    }
  });
  progressBar.animate(1.0, {
    duration: 800
  }, function() {
    $('#headerload').html('')
  });



</script>
