      <h5 class="display-3">View all via Ajax</h5>
      <p>Deploy the template from ajax call.</p>

      <div class="alert alert-secondary" role="alert">
        This group item id is : <strong>viewallajax</strong>
      </div>
      <div style="position: absolute;top:5px;right: 5px">            
        {{>issuebutton parentid="viewallajax"}}
      </div> 


      <div class="card">
       <div class="card-body">
        <h5>Demo</h5><br>
        <div id="{{ids}}_2">
          <button href="" class="btn btn-success" onclick="viewallajax('{{ids}}_2')">Call with Ajax</button>
        </div>
      </div>
    </div>
    
    <script type="text/javascript">

      function viewallajax(location) {
        jQuery('#'+location).html('<i class="fas fa-spinner fa-spin"></i>')
        jQuery.ajax({
          'async': true,
          'global': false,
          'url': '/issues/ajaxview/',
          'success': function (dataSchemaObject) {
            jQuery('#'+location).html(dataSchemaObject)
          }
        });
      }
    </script>